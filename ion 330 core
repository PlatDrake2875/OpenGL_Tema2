[32m+[m[32m#include "PlayerMovement.h"[m
[32m+[m
[32m+[m[32mModel* PlayerMovement::getPlayer()[m
[32m+[m[32m{[m
[32m+[m	[32mreturn player;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvoid PlayerMovement::setPlayer(Model* _player)[m
[32m+[m[32m{[m
[32m+[m	[32mif (_player == nullptr)[m
[32m+[m		[32mreturn;[m
[32m+[m	[32mplayer = _player;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvoid PlayerMovement::ProcessNormalKeys(unsigned char key, int x, int y)[m
[32m+[m[32m{[m
[32m+[m	[32mswitch(key){[m
[32m+[m	[32mcase 'w':[m
[32m+[m		[32mdir = glm::vec3(-1.f, 0.f, 0.f);[m
[32m+[m		[32mquaternionDir = glm::vec3(0.f, 1.f, 0.f);[m
[32m+[m		[32mbreak;[m
[32m+[m	[32mcase 's':[m
[32m+[m		[32mdir = glm::vec3(1.f, 0.f, 0.f);[m
[32m+[m		[32mquaternionDir = glm::vec3(0.f, -1.f, 0.f);[m
[32m+[m		[32mbreak;[m
[32m+[m	[32mcase 'a':[m
[32m+[m		[32mdir = glm::vec3(0.f, -1.f, 0.f);[m
[32m+[m		[32mquaternionDir = glm::vec3(1.f, 0.f, 0.f);[m
[32m+[m		[32mbreak;[m
[32m+[m	[32mcase 'd':[m
[32m+[m		[32mdir = glm::vec3(0.f, 1.f, 0.f);[m
[32m+[m		[32mquaternionDir = glm::vec3(-1.f, 0.f, 0.f);[m
[32m+[m		[32mbreak;[m
[32m+[m	[32mcase ' ':[m
[32m+[m		[32mdir = glm::vec3(0.f, 0.f, 1.f);[m
[32m+[m		[32mbreak;[m
[32m+[m	[32m}[m
[32m+[m	[32mupdatePlayerPosition();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/// <summary>[m
[32m+[m[32m/// Move the Oreo in the wanted direction and rotate it[m
[32m+[m[32m/// </summary>[m
[32m+[m[32mvoid PlayerMovement::updatePlayerPosition()[m
[32m+[m[32m{[m
[32m+[m	[32mplayer->rotateThenTranslate(5.f, quaternionDir, dir);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
