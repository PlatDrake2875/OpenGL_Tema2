[32m+[m[32m//! Update both the verices data and the OpenGL buffers[m
[32m+[m[32mvoid Mesh::setVertices(const std::vector<Vertex>& newVertices) {[m
[32m+[m[32m    vertices = newVertices;[m[41m  [m
[32m+[m
[32m+[m[32m    glBindVertexArray(VAO);[m
[32m+[m
[32m+[m[32m    glBindBuffer(GL_ARRAY_BUFFER, VBO);[m
[32m+[m[32m    glBufferData(GL_ARRAY_BUFFER, vertices.size() * sizeof(Vertex), &vertices[0], GL_STATIC_DRAW);[m
[32m+[m
[32m+[m[32m    glEnableVertexAttribArray(0);[m
[32m+[m[32m    glVertexAttribPointer(0, 3, GL_FLOAT, GL_FALSE, sizeof(Vertex), (void*)offsetof(Vertex, Position));[m
[32m+[m
[32m+[m[32m    glEnableVertexAttribArray(1);[m
[32m+[m[32m    glVertexAttribPointer(1, 3, GL_FLOAT, GL_FALSE, sizeof(Vertex), (void*)offsetof(Vertex, Normal));[m
[32m+[m
[32m+[m[32m    glEnableVertexAttribArray(2);[m
[32m+[m[32m    glVertexAttribPointer(2, 2, GL_FLOAT, GL_FALSE, sizeof(Vertex), (void*)offsetof(Vertex, TexCoords));[m
[32m+[m
[32m+[m[32m    glBindVertexArray(0);[m
[32m+[m[32m}[m
