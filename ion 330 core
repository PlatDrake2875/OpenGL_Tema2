[36m@@ -0,0 +1,14 @@[m
[32m+[m[32m#include "Plane.h"[m
[32m+[m
[32m+[m[32mPlane::Plane(const glm::vec4& p1, const glm::vec4& p2, const glm::vec4& p3, const glm::vec4& p4) {[m
[32m+[m	[32mpoint1 = p1;[m
[32m+[m	[32mpoint2 = p2;[m
[32m+[m	[32mpoint3 = p3;[m
[32m+[m	[32mpoint4 = p4;[m
[32m+[m	[32ma = p1.y * p2.z + p2.y * p3.z + p3.y * p1.z - p3.y * p2.z - p1.y * p3.z - p2.y * p1.z;[m
[32m+[m	[32mb = -(p1.x * p2.z + p2.x * p3.z + p3.x * p1.z - p3.x * p2.z - p1.x * p3.z - p2.x * p1.z);[m
[32m+[m	[32mc = p1.x * p2.y + p2.x * p3.y + p3.x * p1.y - p3.x * p2.y - p1.x * p3.y - p2.x * p1.y;[m
[32m+[m	[32md = -(p1.x * p2.y * p3.z + p2.x * p3.y * p1.z + p3.x * p1.y * p2.z -[m
[32m+[m		[32mp3.x * p2.y * p1.z - p1.x * p3.y * p2.z - p2.x * p1.y * p3.z);[m
[32m+[m[32m}[m
[32m+[m
