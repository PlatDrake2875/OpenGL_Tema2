[32m+[m[32mvoid Model::setMeshesVertices(const std::vector<std::vector<Vertex>>& newMeshesVertices) {[m
[32m+[m	[32mif (meshes.size() != newMeshesVertices.size()) {[m
[32m+[m		[32mstd::cerr << "Error: Mismatch in number of meshes and vertex sets provided." << std::endl;[m
[32m+[m		[32mreturn;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfor (size_t i = 0; i < meshes.size(); ++i) {[m
[32m+[m		[32mmeshes[i].setVertices(newMeshesVertices[i]);[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvoid Model::updateModelMatrix() {[m
[32m+[m	[32m// Get the location of the 'model' uniform in the shader[m
[32m+[m	[32mGLuint modelMatrixLocation = glGetUniformLocation(shaderProgram, "model");[m
[32m+[m
[32m+[m	[32m// Update the 'model' uniform in the shader with the current modelMatrix[m
[32m+[m	[32mglUniformMatrix4fv(modelMatrixLocation, 1, GL_FALSE, glm::value_ptr(modelMatrix));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/// <summary>[m
[32m+[m[32m/// Translates model using the dir vector[m
[32m+[m[32m/// </summary>[m
[32m+[m[32m/// <param name="dir">direction vector</param>[m
[32m+[m[32mvoid Model::translate(glm::vec3 dir) {[m
[32m+[m	[32m/*glm::vec4 lastColumn = modelMatrix[3];[m
[32m+[m	[32mmodelMatrix = glm::mat4(1.0f);[m
[32m+[m	[32mmodelMatrix[3] = lastColumn;*/[m
[32m+[m
[32m+[m	[32mmodelMatrix = glm::translate(modelMatrix, dir);[m[41m [m
[32m+[m	[32mupdateModelMatrix();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m/// <summary>[m
[32m+[m[32m/// Rotation method for a model using quaternions[m
[32m+[m[32m/// </summary>[m
[32m+[m[32m/// <param name="deg"> degrees (0-360) </param>[m
[32m+[m[32m/// <param name="dir"> direction vector </param>[m
[32m+[m[32mvoid Model::rotate(GLfloat deg, glm::vec3 axis) {[m
[32m+[m	[32m//std::cerr << glm::to_string(modelMatrix) << '\n';[m
[32m+[m	[32m//std::cerr << "cum rot: " << glm::to_string(cumulativeRotation) << '\n';[m
[32m+[m	[32mglm::vec3 center = calculateModelCenter();[m
[32m+[m
[32m+[m	[32mfloat radians = glm::radians(deg);[m
[32m+[m	[32mglm::quat quaternionRotation = glm::angleAxis(radians, glm::normalize(axis));[m
[32m+[m	[32m//std::cerr << "quat rot: " << glm::to_string(quaternionRotation) << '\n';[m
[32m+[m
[32m+[m	[32mglm::mat4 translationToCenter = glm::translate(glm::mat4(1.0f), -center);[m
[32m+[m	[32mglm::mat4 rotationMatrix = glm::toMat4(quaternionRotation);[m
[32m+[m	[32mglm::mat4 translationBack = glm::translate(glm::mat4(1.0f), center);[m
[32m+[m
[32m+[m	[32m//std::cerr << glm::to_string(translationToCenter) << '\n';[m
[32m+[m	[32m//std::cerr << glm::to_string(translationBack) << '\n';[m
[32m+[m	[32m//std::cerr << '\n';[m
[32m+[m
[32m+[m	[32mmodelMatrix = translationBack * rotationMatrix * translationToCenter * modelMatrix;[m
[32m+[m
[32m+[m	[32mupdateModelMatrix();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvoid Model::scale(GLfloat scaleFactor) {[m
[32m+[m	[32mmodelMatrix = glm::scale(modelMatrix, glm::vec3(scaleFactor, scaleFactor, scaleFactor));[m[41m [m
[32m+[m	[32mupdateModelMatrix();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvoid Model::rotateThenTranslate(GLfloat deg, glm::vec3 axis, glm::vec3 dir)[m
[32m+[m[32m{[m
[32m+[m	[32mfloat radians = glm::radians(deg);[m
[32m+[m	[32mglm::quat quaternionRotation = glm::angleAxis(radians, glm::normalize(axis));[m
[32m+[m
[32m+[m	[32mglm::mat4 translationMatrix = glm::translate(glm::mat4(1.0f), dir);[m
[32m+[m
[32m+[m	[32mglm::vec3 center = calculateModelCenter();[m
[32m+[m	[32mglm::mat4 translationToCenter = glm::translate(glm::mat4(1.0f), -center);[m
[32m+[m	[32mglm::mat4 translationBack = glm::translate(glm::mat4(1.0f), center);[m
[32m+[m
[32m+[m	[32mglm::mat4 rotationMatrix = glm::toMat4(quaternionRotation);[m
[32m+[m
[32m+[m	[32mmodelMatrix = translationMatrix * modelMatrix; // Apply translation[m
[32m+[m	[32mmodelMatrix = translationBack * rotationMatrix * translationToCenter * modelMatrix; // Apply rotation[m[41m [m
[32m+[m
[32m+[m	[32mstd::cerr << glm::to_string(modelMatrix) << '\n';[m
[32m+[m	[32mupdateModelMatrix();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m/// <summary>[m
[32m+[m[32m/// Computes the center point of a model[m
[32m+[m[32m/// </summary>[m
[32m+[m[32m/// <returns></returns>[m
[32m+[m[32mglm::vec3 Model::calculateModelCenter() const {[m
[32m+[m	[32mglm::vec3 sum(0.0f, 0.0f, 0.0f);[m
[32m+[m	[32mint totalVertices = 0;[m
[32m+[m
[32m+[m	[32mfor (const auto& mesh : meshes) {[m
[32m+[m		[32mfor (const auto& vertex : mesh.vertices) {[m
[32m+[m			[32mglm::vec4 transformedPosition = modelMatrix * glm::vec4(vertex.Position, 1.0f);[m
[32m+[m			[32msum += glm::vec3(transformedPosition);[m
[32m+[m		[32m}[m
[32m+[m		[32mtotalVertices += mesh.vertices.size();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif (totalVertices > 0) {[m
[32m+[m		[32msum /= static_cast<float>(totalVertices);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn sum;[m
[32m+[m[32m}[m
[32m+[m
 void Model::loadModel(std::string const& path)[m
 {[m
