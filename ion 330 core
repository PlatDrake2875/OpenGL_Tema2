[32m+[m		[32mstd::cerr << "Bro what u doin? You're out of bounds.\n";[m
[32m+[m		[32mreturn nullptr;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn models[index];[m
[32m+[m[32m}[m
[32m+[m
